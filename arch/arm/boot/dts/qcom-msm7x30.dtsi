/dts-v1/;

/include/ "skeleton.dtsi"

/ {
	model = "Qualcomm MSM7x30";
	compatible = "qcom,msm7x30";
	interrupt-parent = <&intc>;

	intc: interrupt-controller@C0080000 {
		compatible = "qcom,msm-vic";
		interrupt-controller;
		#interrupt-cells = <2>;
		reg = <0xC0080000 0x1000>;
		num-irqs = <128>;
	};

	msmgpio: gpio-controller {
		compatible = "qcom,msm-gpio-v1";
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <50 0>, <51 0>;
	};

	msmhandset {
		compatible = "qcom,msm-handset";
		hs-name = "7k_handset";
		pwr-key-delay-ms = <500>; /* 0 will disable end key */
	};
};
